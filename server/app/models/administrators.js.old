var mongoose, carSchema, Schema, modelName, collectionName;

/* Include dependencies */
mongoose = require('mongoose');
Schema = mongoose.Schema;

/* Schema definition */
adminSchema = Schema({
    username: {type: String, required: true},
    password: {type: String, required: true}
});

modelName = "Administrator";
collectionName = "administrators"; // Naming convention is plural.
mongoose.model(modelName, adminSchema, collectionName);
